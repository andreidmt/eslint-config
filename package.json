{
  "name": "@mutant-ws/eslint-config",
  "version": "5.0.0",
  "description": "JavaScript ESLint rules",
  "homepage": "https://github.com/mutant-ws/eslint-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mutant-ws/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/mutant-ws/eslint-config/issues"
  },
  "main": "targets/node.js",
  "scripts": {
    "----UTIL": "",
    "setup": "rm -rf ./node_modules && npm i && npm audit fix",
    "setup:ci": "npm ci && npm audit fix",
    "update": "npm-check --update --save-exact",
    "----TEST": "",
    "lint:js": "eslint rules/* targets/*",
    "lint:md": "markdownlint *.md",
    "lint": "npm run lint:js && npm run lint:md",
    "----BUILD": "",
    "prepublishOnly": "npm audit fix && npm run lint"
  },
  "keywords": [
    "config",
    "eslint",
    "eslint-config",
    "eslint-rules",
    "javascript",
    "prettier",
    "react",
    "node"
  ],
  "author": {
    "name": "Andrei Dumitrescu",
    "url": "https://github.com/andrei-dumitrescu"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-html": "6.0.2",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-no-inferred-method-name": "^2.0.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "4.0.0",
    "eslint-plugin-unicorn": "19.0.1",
    "markdownlint-cli": "^0.22.0",
    "npm-check": "^5.9.2",
    "prettier": "2.0.5",
    "semantic-release": "17.0.7"
  },
  "peerDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-no-inferred-method-name": "^2.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-unicorn": "^19.0.1",
    "prettier": "^2.0.5"
  },
  "engines": {
    "node": ">= 6"
  },
  "license": "MIT"
}
