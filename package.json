{
  "name": "@asd14/eslint-config",
  "version": "3.0.2",
  "description": "JavaScript ESLint rules",
  "homepage": "https://github.com/asd14/eslint-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asd14/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/asd14/eslint-config/issues"
  },
  "main": "targets/node.js",
  "scripts": {
    "----UTILS------------------------------------": "",
    "setup": "rm -rf ./node_modules && npm i && npm audit fix",
    "update": "npm-check -u",
    "----TEST-------------------------------------": "",
    "lint:js": "eslint ./rules/*",
    "lint:md": "markdownlint *.md",
    "lint": "npm run lint:js && npm run lint:md",
    "----BUILD------------------------------------": "",
    "prepare": "npm run lint",
    "preversion": "npm run prepare"
  },
  "keywords": [
    "asd14",
    "config",
    "eslint",
    "eslint-config",
    "eslint-rules",
    "flow",
    "flowtype",
    "javascript",
    "prettier",
    "react"
  ],
  "author": {
    "name": "Andrei Dumitrescu",
    "url": "https://github.com/andreidcm"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "5.9.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-compat": "2.6.3",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-flowtype-errors": "3.6.0",
    "eslint-plugin-html": "5.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-json": "1.2.1",
    "eslint-plugin-jsx-control-statements": "2.2.1",
    "eslint-plugin-no-inferred-method-name": "1.0.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-unicorn": "6.0.1",
    "flow-bin": "0.86.0",
    "markdownlint-cli": "0.13.0",
    "npm-check": "5.9.0",
    "prettier": "1.15.2"
  },
  "peerDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-json": "^1.2.1",
    "eslint-plugin-no-inferred-method-name": "^1.0.2",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-unicorn": "^6.0.1",
    "prettier": "^1.15.2"
  },
  "engines": {
    "node": ">= 6"
  },
  "license": "MIT"
}
