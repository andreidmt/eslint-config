{
  "name": "@mutantlove/eslint-config",
  "version": "5.0.0",
  "description": "JavaScript ESLint rules",
  "homepage": "https://github.com/mutantlove/eslint-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mutantlove/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/mutantlove/eslint-config/issues"
  },
  "main": "targets/node.js",
  "scripts": {
    "----UTIL": "",
    "setup": "rm -rf ./node_modules && npm i && npm audit fix",
    "setup:ci": "npm ci && npm audit fix",
    "update": "npm-check --update --save-exact",
    "----TEST": "",
    "lint:js": "eslint rules/* targets/*",
    "lint:md": "markdownlint *.md",
    "lint": "npm run lint:js && npm run lint:md",
    "----BUILD": "",
    "prepublishOnly": "npm audit fix && npm run lint"
  },
  "keywords": [
    "config",
    "eslint",
    "eslint-config",
    "eslint-rules",
    "javascript",
    "prettier",
    "react",
    "node"
  ],
  "author": {
    "name": "Andrei Dumitrescu",
    "url": "https://github.com/andreidcm"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "6.5.1",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-html": "6.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-json": "1.4.0",
    "eslint-plugin-no-inferred-method-name": "1.0.2",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-unicorn": "12.0.2",
    "markdownlint-cli": "0.18.0",
    "npm-check": "5.9.0",
    "prettier": "1.18.2",
    "semantic-release": "15.13.24"
  },
  "peerDependencies": {
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-no-inferred-method-name": "^1.0.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-unicorn": "^12.0.2",
    "prettier": "^1.18.2"
  },
  "engines": {
    "node": ">= 6"
  },
  "license": "MIT"
}
